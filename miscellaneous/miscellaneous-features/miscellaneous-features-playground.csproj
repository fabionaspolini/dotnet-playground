<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <LangVersion>10</LangVersion>
    <RootNamespace>miscellaneous_features_playground</RootNamespace>
    <CodeAnalysisRuleSet>codeanalysis.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>

  <!-- Enable source disk file write to correct IDE's works -->
  <PropertyGroup>
    <CompilerGeneratedFilesOutputPath>$(MSBuildProjectDirectory)/../generated/$(MSBuildProjectName)</CompilerGeneratedFilesOutputPath>
    <EmitCompilerGeneratedFiles>false</EmitCompilerGeneratedFiles>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DasMulli.DataBuilderGenerator" Version="2.0.0" />
    <PackageReference Include="DasMulli.DataBuilderGenerator.Attributes" Version="2.0.0" />
    <PackageReference Include="FluentDateTime" Version="3.0.0" />
    <PackageReference Include="Maestria.Extensions" Version="3.5.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="9.0.8" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="9.0.8" />
    <PackageReference Include="Microsoft.Extensions.Hosting" Version="9.0.8" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="9.0.8" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="9.0.8" />
    <PackageReference Include="PrimaryConstructor" Version="1.4.0" />
  </ItemGroup>

</Project>
