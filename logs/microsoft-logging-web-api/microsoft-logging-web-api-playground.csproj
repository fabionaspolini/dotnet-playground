<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>microsoft_logging_web_api_playground</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\microsoft-logging\MyJsonConsoleFormatter\MyJsonConsoleFormatter.cs" Link="MyJsonConsoleFormatter\MyJsonConsoleFormatter.cs" />
    <Compile Include="..\microsoft-logging\MyJsonConsoleFormatter\MyJsonConsoleFormatterHelpers.cs" Link="MyJsonConsoleFormatter\MyJsonConsoleFormatterHelpers.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="7.0.7" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.5.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="MyLogger\" />
  </ItemGroup>

</Project>
