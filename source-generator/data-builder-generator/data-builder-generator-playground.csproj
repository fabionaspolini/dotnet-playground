<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <RootNamespace>data_builder_generator_playground</RootNamespace>
    <!-- <Nullable>Enable</Nullable> -->
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DasMulli.DataBuilderGenerator" Version="2.0.0" />
    <PackageReference Include="DasMulli.DataBuilderGenerator.Attributes" Version="2.0.0" />
  </ItemGroup>

  <!-- Enable source disk file write to correct IDE's works -->
  <PropertyGroup>
    <CompilerGeneratedFilesOutputPath>$(MSBuildProjectDirectory)/../generated/$(MSBuildProjectName)</CompilerGeneratedFilesOutputPath>
    <EmitCompilerGeneratedFiles>false</EmitCompilerGeneratedFiles>
  </PropertyGroup>

</Project>
